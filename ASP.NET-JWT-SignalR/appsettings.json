{

  "KeyVaultUrl": "",
  
  "AllowedHosts": "*",
  "JwtToken":"LlamaEngineHostToken",
  
  "JwtOptions": {
    "Key": "",
    "Issuer": "https://localhost:5231",
    "Audience": "https://localhost:5231",
    "ExpireDays": "1"
  },

  
  "ApiUrls": {
    "Base": "http://TheOutSideAPI/"
  },

  "Serilog": {
  "MinimumLevel": { "Default": "Information", "Override": { "Microsoft": "Warning", "Microsoft.AspNetCore": "Warning", "System": "Warning" } },
  "WriteTo": [
    {
      "Name": "File",
      "Args": {
        "path": "Logs/log-.json",
        "rollingInterval": "Day",
        "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
      }
    }
  ],
  "Enrich": [
    "FromLogContext",
    "WithMachineName",
    "WithThreadId",
    "WithClientIp",
    "WithClientAgent"
  ]
}

}


